/* pages/component/jf/jfscxq/jfscxq.wxss */
@import '../../../common/common.wxss';
.fl{
  float:left;
}
.fr{
  float:right;
}
.clearfix:after {/***加在元素父级清除浮动*****/
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.banner{
  width: 750rpx;
  height: 640rpx;
  display: block;
}
.banner image{
  width: 750rpx;
  height: 640rpx;
  display: block;
}
.detailTit{
  width: 100%;
  padding:0 40rpx;
  height: 165rpx;
  background-color: #fff;
}
.detailTit .tit{
  color: #000000;
  font-size: 28rpx;
  line-height: 30rpx;
}
.detailTit view{
  font-size: 28rpx;
  color: #c2c3c2;
  padding-bottom: 5rpx;
  height: 60rpx;
}
.detailTit view .price{
  color: #ed2b28;
  font-size: 30repx;
  margin-left:0;
}
.detailTit view text{
  margin-left:35rpx;
}
.h-15{
  height: 15rpx;
  background-color: #f7f7f7;
}
/***图文详情****/
.image-textw .titw{
  width: 275rpx;
  height: 85rpx;
  text-align: center;
  line-height: 35rpx;
  font-size: 30rpx;
  color: #616161;
  margin: 0 auto;
  padding: 25rpx 0;
}
.image-textw .titw .xian{
  width: 65rpx;
  height: 2rpx;
  background: #e5e5e5;
  margin-top: 16rpx;
}
.image-textw .imgshow{
  width: 750rpx;
}
.image-textw .imgshow image{
  display: block;
  width: 100%;
}

.detailbot navigator{
  float:left;
}
.detailk{
  height: 110rpx;
}
.detailbot{
  width: 100%;
  height: 110rpx;
  position: fixed;
  bottom: 0;
  background-color: #fff;
}
.detailbot .sc{
  width: 100rpx;
  border-right: 1px solid #e5e5e5;
  text-align: center;
  font-size: 21rpx;
  color: #646664;
  line-height: 25rpx;
  overflow: hidden;
  height: 110rpx;
}
.detailbot .sc view{
  width: 45rpx;
  height: 45rpx;
  margin: 15rpx auto 0;
}
.detailbot navigator image{
  width: 45rpx;
  height: 45rpx;
  display: block;
}
.detailbot .jf{
  width: 100rpx;
  text-align: center;
  font-size: 21rpx;
  color: #646664;
  line-height: 25rpx;
  overflow: hidden;
  height: 110rpx;
}
.detailbot .jf view{
  width: 45rpx;
  height: 45rpx;
  margin: 15rpx auto 0;
}
.detailbot .ljdh{
   width: 540rpx;
  height: 110rpx;
  text-align: center;
  font-size: 35rpx;
  color: #ffffff;
  background-color: #000000;
  line-height: 110rpx;
  border-radius: 0;
  outline: 0;
  padding: 0;
  display: block;
  float: right;
  margin: 0;
  background: #23242a;
}