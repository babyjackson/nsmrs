.recommendxg{
  padding: 15rpx 20rpx 0 20rpx;
  background: #f0efef;
}
.recommendxg view-item{
  width: 330rpx;
  background: #fff;
  margin-bottom: 15rpx;
  height: 500rpx;
  padding: 10rpx;
}
.recommendxg view-item:nth-of-type(odd){
  float: left;
}
.recommendxg view-item:nth-of-type(even){
  float: right;
}
.recommendxg view-item .img{
  width: 330rpx;
  height: 350rpx;
}
.recommendxg view-item image{
  width: 330rpx;
  height: 350rpx;
}
.recommendxg view-item .tit{
  font-size: 22rpx;
  color: #212121;
  line-height: 34rpx;
  margin-top: 15rpx;
}
.recommendxg view-item .pj{
  width: 330rpx;
  height: 36rpx;
  border:2rpx solid #23242a;
  display: flex;
  font-size: 24rpx;
  margin-top: 15rpx;
}
.recommendxg view-item .pj view{
  width: 50%;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  font-size: 24rpx;
  color:#23242a; 
}
.recommendxg view-item .pj button{
  width: 50%;
  outline: 0;
  border: 0;
  display: block;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  color: #fff;
  border-radius: 0;
  background: #23242a;
  font-size: 24rpx;
  }
