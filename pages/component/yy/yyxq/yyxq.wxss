    color: #9d9c9c;
    margin-bottom: 25rpx;
    display: flex;
    }  
.detailTab view{  
  font-size: 30rpx;  
  display: inline-block;  
  width: 33.33%;  
  border-bottom: 4rpx solid #fff;
}  
.detailTab .active{ 
  color: #23242a;  
